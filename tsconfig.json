{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "baseUrl": ".",
  "paths": {
    "@/*": [
      "./src/*"
    ]
  },
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "./",
    "paths": {
      "@shadcn/ui": ["node_modules/@shadcn/ui"]
    }
  },
  
}